<keymap version="1" name="Default for Windows copy" parent="$default">
  <action id="CloseActiveTab">
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorDuplicate" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="shift ctrl f" />
  </action>
</keymap>